<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.matriculacion">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".alumnodetalleactivity.ActivityAlumnosDetalle"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity android:name=".asignaturaactivity.ActivityAsignaturas"
            android:label="Lista Asignaturas"/>
        <activity android:name=".alumnoactivity.ActivityAlumnos"
            android:label="Lista Alumnos"
            />
        <activity android:name=".mainactivity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>